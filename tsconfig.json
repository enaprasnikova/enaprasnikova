{
	"compilerOptions": {
	  "baseUrl": ".",
	  "paths": {
		"src/*": ["src/*"]
	  },
	  "lib": ["dom", "dom.iterable", "esnext"],
	  "allowJs": true,
	  "allowSyntheticDefaultImports": true,
	  "skipLibCheck": true,
	  "esModuleInterop": true,
	  "target": "ES2015",
	  "module": "commonjs",
	  "sourceMap": true,
	  "noImplicitAny": true,
	  "downlevelIteration": true,
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "jsx": "react"
	}
}
